<%- include('ek/header') %>

  <head>
 <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
 <meta name="msapplication-TileColor" content="#4D77CD">
 <meta name="msapplication-TileImage" content="<%=bot.user.avatarURL%>">
 <meta name="theme-color" content="#4D77CD">
 <meta name="viewport" content="width=device-width, initial-scale=1" />
 <meta name="description" content="Spice up your Discord experience with our diverse range of Discord bots">
 <meta name="keywords" content="Bots, Discord, English">
 <meta property="og:title" content="Discord4Bots" />
 <meta property="og:type" content="Website" />
 <meta property="og:url" content="" />
 <meta property="og:description" content="Spice up your Discord experience with our diverse range of Discord bots" />
 <meta property="og:image" content="<%=bot.user.avatarURL%>" />
 <meta itemprop="description" content="Spice up your Discord experience with our diverse range of Discord bots">
 <meta itemprop="image" content="<%=bot.user.avatarURL%>">
<head>

  <div align="center">
      <div class="jumbotron">
        <% for (var i = 0; i < Object.keys(bot.profile); i++) { %>
          <% var id = Object.keys(bot.profile)[i] %>
          <% if (bot.profile[id]) { %>
        <img src="<%=bot.profile[id].avatar%>" class="rounded-circle">
        <br>
        <a href="/user/<%=id%>" class="btn btn-default" style="background-color:#333; color:#fff"><%=bot.profil[id].tag%> Open Your Profile Page!</a>
        <br>
        <a href="/user/<%=id%>/profile" class="btn btn-default" style="background-color:#333; color:#fff"><%=bot.profil[i].tag%> Open Your Profile Page!</a>
        <br><br>
          <% } %>
        <% } %>
      </div>
  </div>

<%- include ./ek/footer %>